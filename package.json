{
  "name": "object-properties",
  "version": "2.0.0",
  "description": "Shortcuts to Object.defineProperty and Object.defineProperties",
  "keywords": ["Object.defineProperties", "Object.defineProperty"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/object-properties.git",
  "homepage": "https://github.com/christophehurpeau/object-properties#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/object-properties/issues"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "lib/index.js",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index.cjs.js",
  "browser-dev": "./dist/index-dev.cjs.js",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build",
    "clean": "rm -Rf docs dist",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "flow": "flow",
    "generate:test-coverage":
      "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "eslint src/",
    "postcheckout": "yarnhook",
    "postmerge": "yarnhook",
    "postpublish": "make post-publish",
    "postrewrite": "yarnhook",
    "precommit": "lint-staged",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": ["prettier --write", "git add"],
    "src/**/*.json": ["prettier --write", "git add"],
    "src/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": ["<rootDir>/src/**/__tests__/**/*.js", "<rootDir>/src/**/*.test.js"],
    "collectCoverageFrom": ["src/**/*.js"]
  },
  "dependencies": {
    "flow-runtime": "^0.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest-node": "^1.0.0",
    "babel-preset-modern-browsers": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^18.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.68.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^7.0.0",
    "pob-babel": "^20.1.1",
    "pob-release": "^3.1.0",
    "prettier": "^1.11.1",
    "proclaim": "^3.4.1",
    "xunit-file": "1.0.0",
    "yarnhook": "^0.1.1"
  }
}
