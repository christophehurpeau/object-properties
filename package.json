{
  "name": "object-properties",
  "version": "6.1.0",
  "description": "Shortcuts to Object.defineProperty and Object.defineProperties",
  "keywords": [
    "Object.defineProperties",
    "Object.defineProperty"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/object-properties.git",
  "homepage": "https://github.com/christophehurpeau/object-properties#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/object-properties/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.4",
  "browserslist": [
    "defaults",
    "> 0.2%",
    "not ie < 12",
    "not safari < 10",
    "not ios_saf < 10"
  ],
  "main": "./dist/index-node16.mjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "node": {
        "import": "./dist/index-node16.mjs",
        "require": "./dist/index-node16.cjs"
      },
      "browser": {
        "browser:modern": {
          "import": "./dist/index-browsermodern.es.js"
        },
        "import": "./dist/index-browser.es.js",
        "require": "./dist/index-browser.cjs.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.build.json",
    "checks": "node scripts/check-package.js",
    "clean": "yarn clean:build",
    "clean:build": "pob-babel-clean-out dist",
    "generate:api": "typedoc --tsconfig tsconfig.doc.json",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn run lint:prettier && tsc && yarn run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet *.{js,cjs,mjs} src scripts",
    "lint:prettier": "pob-root-prettier --check .",
    "lint:prettier:fix": "pob-root-prettier --write .",
    "postinstallDev": "pob-root-postinstall",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts",
      "<rootDir>/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "pob": {
    "babelEnvs": [
      {
        "target": "node",
        "version": "16",
        "formats": [
          "cjs",
          "es"
        ]
      },
      {
        "target": "browser",
        "version": "modern",
        "formats": [
          "es"
        ]
      },
      {
        "target": "browser",
        "formats": [
          "cjs",
          "es"
        ]
      }
    ],
    "entries": [
      "index"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@pob/commitlint-config": "5.0.0",
    "@pob/eslint-config": "49.4.0",
    "@pob/eslint-config-typescript": "49.4.0",
    "@pob/root": "7.1.1",
    "@types/jest": "29.2.3",
    "babel-preset-modern-browsers": "15.0.2",
    "check-package-dependencies": "3.10.0",
    "eslint": "8.27.0",
    "jest": "29.3.1",
    "pob-babel": "35.2.0",
    "typedoc": "0.23.21",
    "typescript": "4.9.3"
  }
}
