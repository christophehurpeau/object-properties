{
  "name": "object-properties",
  "version": "3.1.0",
  "description": "Shortcuts to Object.defineProperty and Object.defineProperties",
  "keywords": ["Object.defineProperties", "Object.defineProperty"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/object-properties.git",
  "homepage": "https://github.com/christophehurpeau/object-properties#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/object-properties/issues"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "lib/index.js",
  "typings": "./dist/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "module-dev": "./dist/index-browser-dev.es.js",
  "browser": "./dist/index-browser.cjs.js",
  "browser-dev": "./dist/index-browser-dev.cjs.js",
  "module:node": "./dist/index-node8.es.js",
  "module:node-dev": "./dist/index-node8-dev.es.js",
  "module:browser": "./dist/index-browser.es.js",
  "module:browser-dev": "./dist/index-browser-dev.es.js",
  "module:modern-browsers": "./dist/index-browsermodern.es.js",
  "module:modern-browsers-dev": "./dist/index-browsermodern-dev.es.js",
  "scripts": {
    "build": "pob-build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "clean": "rm -Rf docs dist",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "generate:test-coverage":
      "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "tsc --noEmit && eslint --ext .ts  src/",
    "postcheckout": "yarnhook",
    "postmerge": "yarnhook",
    "postpublish": "make post-publish",
    "postrewrite": "yarnhook",
    "precommit": "lint-staged",
    "preversion": "yarn run lint && yarn run build && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version",
    "watch": "pob-watch"
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": ["prettier --write", "git add"],
    "src/**/*.json": ["prettier --write", "git add"],
    "src/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": ["<rootDir>/src/**/__tests__/**/*.ts", "<rootDir>/src/**/*.test.ts"],
    "collectCoverageFrom": ["src/**/*.ts"],
    "moduleFileExtensions": ["ts", "js"],
    "transform": {
      "^.+\\.ts$": "babel-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^22.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-latest-node": "^2.0.0-beta.2",
    "babel-preset-modern-browsers": "^12.0.0-beta.0",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^19.0.2",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "^0.11.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^7.0.4",
    "pob-babel": "^21.2.4",
    "pob-release": "^4.0.0",
    "prettier": "^1.12.1",
    "proclaim": "^3.4.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "xunit-file": "1.0.0",
    "yarnhook": "^0.2.0"
  }
}
