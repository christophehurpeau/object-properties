{
  "name": "object-properties",
  "version": "9.0.1",
  "description": "Shortcuts to Object.defineProperty and Object.defineProperties",
  "keywords": [
    "Object.defineProperties",
    "Object.defineProperty"
  ],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/object-properties.git",
  "homepage": "https://github.com/christophehurpeau/object-properties#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/object-properties/issues"
  },
  "type": "module",
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=22.18.0"
  },
  "sideEffects": false,
  "main": "./dist/index-node20.mjs",
  "types": "./dist/definitions/index.d.ts",
  "module": "./dist/index-browser.es.js",
  "browser": "./dist/index-browser.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/definitions/index.d.ts",
      "node": {
        "import": "./dist/index-node20.mjs"
      },
      "browser": {
        "import": "./dist/index-browser.es.js"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "yarn clean:build && rollup --config rollup.config.mjs && yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "checks": "node scripts/check-package.js",
    "clean": "yarn clean:build",
    "clean:build": "pob-esbuild-clean-out dist",
    "generate:api": "typedoc --tsconfig tsconfig.doc.json",
    "generate:test-coverage": "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=pob-lcov-reporter --coverageDirectory=docs/coverage/",
    "lint": "yarn run lint:prettier && tsc && yarn run lint:eslint",
    "lint:eslint": "eslint --quiet .",
    "lint:prettier": "pob-root-prettier --check .",
    "lint:prettier:fix": "pob-root-prettier --write .",
    "postinstall": "pob-root-postinstall",
    "postpack": "pinst --enable",
    "prepack": "pinst --disable",
    "test": "TZ=UTC node  --test src/**/*.test.ts",
    "test:coverage": "TZ=UTC npx c8 --all --src ./src node  --test src/**/*.test.ts",
    "test:coverage:json": "TZ=UTC npx c8 --reporter=json --all --src ./src node  --test src/**/*.test.ts",
    "test:watch": "TZ=UTC node  --test src/**/*.test.ts",
    "watch": "yarn clean:build && rollup --config rollup.config.mjs --watch"
  },
  "browserslist": {
    "production": [
      "defaults",
      "> 0.2%",
      "not ie < 12",
      "not safari < 10",
      "not ios_saf < 10"
    ]
  },
  "commitlint": {
    "extends": [
      "@pob/root/commitlint-config"
    ]
  },
  "pob": {
    "bundler": "rollup-esbuild",
    "entries": [
      "index"
    ],
    "envs": [
      {
        "target": "node",
        "version": "22"
      },
      {
        "target": "browser"
      }
    ],
    "typescript": true
  },
  "prettier": "@pob/root/prettier-config",
  "devDependencies": {
    "@pob/eslint-config": "62.0.0",
    "@pob/eslint-config-typescript": "62.0.0",
    "@pob/rollup-esbuild": "7.3.0",
    "@pob/root": "19.5.0",
    "@types/node": "22.18.13",
    "check-package-dependencies": "10.5.0",
    "eslint": "9.39.0",
    "pinst": "3.0.0",
    "typedoc": "0.28.14",
    "typescript": "5.9.3"
  }
}
